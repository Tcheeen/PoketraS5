<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Accueil</servlet-name>
        <servlet-class>controller.Accueil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeMaterielServlet</servlet-name>
        <servlet-class>controller.ListeMaterielServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajout_materielServlet</servlet-name>
        <servlet-class>controller.Ajout_materielServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajout_matiereServlet</servlet-name>
        <servlet-class>controller.Ajout_matiereServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeLook</servlet-name>
        <servlet-class>controller.ListeLook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajoute_employe</servlet-name>
        <servlet-class>controller.Ajoute_employe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MatierePServlet</servlet-name>
        <servlet-class>controller.MatierePServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Accueil</servlet-name>
        <url-pattern>/Accueil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeMaterielServlet</servlet-name>
        <url-pattern>/ListeMaterielServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajout_materielServlet</servlet-name>
        <url-pattern>/Ajout_materielServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajout_matiereServlet</servlet-name>
        <url-pattern>/Ajout_matiereServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajoute_employe</servlet-name>
        <url-pattern>/Ajoute_employe</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
